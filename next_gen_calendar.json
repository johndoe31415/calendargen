{
	"meta": {
		"locale":			"de",
		"format":			"30x20",
		"name":				"example"
	},

	"format": {
		"2022-01-01": {
			"text": "Neujahr",
			"starred": true,
			"bgcolor": "#ff0000",
			"fgcolor": "#00ffff"
		},
		"2022-01-10": {
			"bgcolor": "#00ff00"
		}
	},

	"pages": [
		[
			{
				"template": "image_cover_page",
				"transform": {
					"year_text": [
						{
							"cmd": "set_text",
							"text": "2022"
						}
					]
				}
			},
			{
				"template": "header",
				"compose": "inverted",
				"transform": {
					"header_text": [
						{
							"cmd": "set_text",
							"text": "Foobar 1/4"
						}
					]
				}
			}
		],
		[
			{
				"template": "month_calendar",
				"transform": {
					"day_box_01": [
						{
							"cmd": "set_style",
							"fill": "#ff0000",
							"stroke": "#00ff00"
						}
					],
					"group_29": [
						{
							"cmd": "set_style",
							"hide": true
						}
					]
				}
			},
			{
				"template": "landscape_single_image",
				"transform": {
					"image": [
						{
							"cmd": "image",
							"filename": "/tmp/DSCF8633.jpg",
							"gravity": "northwest"
						}
					]
				}
			},
			{
				"template": "header",
				"compose": "inverted",
				"transform": {
					"header_text": [
						{
							"cmd": "set_text",
							"text": "Foobar 2/4"
						}
					]
				}
			}
		]
	]
